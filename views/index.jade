script(type='text/javascript', src='http://www.google.com/jsapi?autoload={\'modules\':[{name:\'maps\',version:3,other_params:\'sensor=false\'}]}')

script(type='text/javascript')
      function init() {
      var locals = !{local};
      var mapDiv = document.getElementById('map');
      var map = new google.maps.Map(mapDiv, {
      center: new google.maps.LatLng( locals.lat, locals.lon),
      zoom: 8,
      mapTypeId: google.maps.MapTypeId.ROADMAP
      });
      }
      google.maps.event.addDomListener(window, 'load', init);


